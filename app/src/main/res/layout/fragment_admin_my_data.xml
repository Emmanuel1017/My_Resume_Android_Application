<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Fragments.AdminMyDataFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <androidx.cardview.widget.CardView
                android:id="@+id/blog_card"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="20dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="20dp"
                android:layout_marginBottom="@dimen/margin_20dp"
                android:clipChildren="false"
                android:clipToPadding="false"
                android:divider="@android:color/transparent"
                android:dividerHeight="0.0px"
                android:elevation="8dp"
                app:cardCornerRadius="@dimen/margin_20dp"
                app:cardElevation="7dp">

                <RelativeLayout
                    android:id="@+id/my_blog_relative_layout"
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:background="@drawable/gradient_cat_sky">

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true">

                            <TextView
                                android:id="@+id/item_title"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/txt_size_20"
                                android:layout_marginTop="@dimen/margin_20dp"
                                android:fontFamily="@font/open_sans_bold"
                                android:gravity="center_vertical"
                                android:maxLines="1"
                                android:text="My Web Blog"
                                android:textColor="@color/white"
                                android:textSize="20sp" />


                            <ImageView
                                android:id="@+id/wave"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/item_title"
                                android:layout_marginStart="@dimen/margin_20dp"
                                android:layout_marginTop="5dp"
                                android:background="@drawable/ic_wave" />

                            <RelativeLayout
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:layout_alignParentEnd="true"
                                android:layout_marginTop="@dimen/margin_20dp"
                                android:layout_marginEnd="@dimen/margin_20dp"
                                android:background="@drawable/shadow_circle"
                                android:padding="7dp">

                                <ImageView
                                    android:id="@+id/my_blog_lock_unlock_img"
                                    android:layout_width="25dp"
                                    android:layout_height="25dp"
                                    android:src="@drawable/unlock_pad" />

                            </RelativeLayout>

                            <ImageView
                                android:id="@+id/my_blog_web_img"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/wave"
                                android:layout_marginStart="@dimen/margin_20dp"
                                android:layout_marginTop="@dimen/margin_10dp"
                                android:layout_marginBottom="@dimen/margin_20dp"
                                android:background="@drawable/web"
                                android:paddingStart="@dimen/margin_10dp"
                                android:paddingTop="5dp"
                                android:paddingEnd="@dimen/margin_10dp"
                                android:paddingBottom="5dp"
                                android:textColor="@color/white"
                                android:textSize="12sp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/noofcate"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/wave"
                                android:layout_marginTop="@dimen/margin_10dp"
                                android:layout_marginBottom="@dimen/margin_20dp"
                                android:layout_toEndOf="@+id/my_blog_web_img"
                                android:paddingStart="@dimen/margin_10dp"
                                android:paddingTop="5dp"
                                android:paddingEnd="@dimen/margin_10dp"
                                android:paddingBottom="5dp"
                                android:text="Earn Through Watching Sponsored Ads"
                                android:textColor="@color/white"
                                android:textSize="12sp"
                                android:textStyle="bold" />

                        </RelativeLayout>

                    </RelativeLayout>
                </RelativeLayout>
            </androidx.cardview.widget.CardView>


            <androidx.cardview.widget.CardView
                android:id="@+id/experiences_card"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="20dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="20dp"
                android:layout_marginBottom="@dimen/margin_20dp"
                android:clipChildren="false"
                android:clipToPadding="false"
                android:divider="@android:color/transparent"
                android:dividerHeight="0.0px"
                android:elevation="8dp"
                app:cardCornerRadius="@dimen/margin_20dp"
                app:cardElevation="7dp">

                <RelativeLayout
                    android:id="@+id/experiences_relative_layout"
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:background="@drawable/gradient_cat_green">

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true">

                            <TextView
                                android:id="@+id/experiences_title"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/txt_size_20"
                                android:layout_marginTop="@dimen/margin_20dp"
                                android:fontFamily="@font/open_sans_bold"
                                android:gravity="center_vertical"
                                android:maxLines="1"
                                android:text="My Web Experiences"
                                android:textColor="@color/white"
                                android:textSize="20sp" />


                            <ImageView
                                android:id="@+id/web_wave"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/experiences_title"
                                android:layout_marginStart="@dimen/margin_20dp"
                                android:layout_marginTop="5dp"
                                android:background="@drawable/ic_wave" />

                            <RelativeLayout
                                android:id="@+id/experiences_lock_unlock_img"
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:layout_alignParentEnd="true"
                                android:layout_marginTop="@dimen/margin_20dp"
                                android:layout_marginEnd="@dimen/margin_20dp"
                                android:background="@drawable/shadow_circle"
                                android:padding="7dp">

                                <ImageView
                                    android:id="@+id/lock_website"
                                    android:layout_width="25dp"
                                    android:layout_height="25dp"
                                    android:src="@drawable/unlock_pad" />

                            </RelativeLayout>

                            <ImageView
                                android:id="@+id/experiences_Web_img"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/web_wave"
                                android:layout_marginStart="@dimen/margin_20dp"
                                android:layout_marginTop="@dimen/margin_10dp"
                                android:layout_marginBottom="@dimen/margin_20dp"
                                android:background="@drawable/web"
                                android:paddingStart="@dimen/margin_10dp"
                                android:paddingTop="5dp"
                                android:paddingEnd="@dimen/margin_10dp"
                                android:paddingBottom="5dp"
                                android:textColor="@color/white"
                                android:textSize="12sp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/my_experiences_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/web_wave"
                                android:layout_marginTop="@dimen/margin_10dp"
                                android:layout_marginBottom="@dimen/margin_20dp"
                                android:layout_toEndOf="@+id/experiences_Web_img"
                                android:paddingStart="@dimen/margin_10dp"
                                android:paddingTop="5dp"
                                android:paddingEnd="@dimen/margin_10dp"
                                android:paddingBottom="5dp"
                                android:text="Manage my Experiences Data"
                                android:textColor="@color/white"
                                android:textSize="12sp"
                                android:textStyle="bold" />

                        </RelativeLayout>

                    </RelativeLayout>
                </RelativeLayout>
            </androidx.cardview.widget.CardView>


            <androidx.cardview.widget.CardView
                android:id="@+id/about_card"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="20dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="20dp"
                android:layout_marginBottom="@dimen/margin_20dp"
                android:clipChildren="false"
                android:clipToPadding="false"
                android:divider="@android:color/transparent"
                android:dividerHeight="0.0px"
                android:elevation="8dp"
                app:cardCornerRadius="@dimen/margin_20dp"
                app:cardElevation="7dp">

                <RelativeLayout
                    android:id="@+id/about_me_relative_layout"
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:background="@drawable/gradient_cat_pink">

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true">

                            <TextView
                                android:id="@+id/about_me_title"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/txt_size_20"
                                android:layout_marginTop="@dimen/margin_20dp"
                                android:fontFamily="@font/open_sans_bold"
                                android:gravity="center_vertical"
                                android:maxLines="1"
                                android:text="My Web About Me"
                                android:textColor="@color/white"
                                android:textSize="20sp" />


                            <ImageView
                                android:id="@+id/daily_wave"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/about_me_title"
                                android:layout_marginStart="@dimen/margin_20dp"
                                android:layout_marginTop="5dp"
                                android:background="@drawable/ic_wave" />

                            <RelativeLayout
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:layout_alignParentEnd="true"
                                android:layout_marginTop="@dimen/margin_20dp"
                                android:layout_marginEnd="@dimen/margin_20dp"
                                android:background="@drawable/shadow_circle"
                                android:padding="7dp">

                                <ImageView
                                    android:id="@+id/about_me_lock_unlock_img"
                                    android:layout_width="25dp"
                                    android:layout_height="25dp"
                                    android:src="@drawable/unlock_pad" />

                            </RelativeLayout>

                            <ImageView
                                android:id="@+id/daily_noofque"
                                android:layout_width="24dp"
                                android:layout_height="24dp"
                                android:layout_below="@+id/daily_wave"
                                android:layout_marginStart="@dimen/margin_20dp"
                                android:layout_marginTop="@dimen/margin_10dp"
                                android:layout_marginBottom="@dimen/margin_20dp"
                                android:background="@drawable/web"
                                android:paddingStart="@dimen/margin_10dp"
                                android:paddingTop="5dp"
                                android:paddingEnd="@dimen/margin_10dp"
                                android:paddingBottom="5dp"
                                android:textColor="@color/white"
                                android:textSize="12sp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/daily_noofcate"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/daily_wave"
                                android:layout_marginTop="@dimen/margin_10dp"
                                android:layout_marginBottom="@dimen/margin_20dp"
                                android:layout_toEndOf="@+id/daily_noofque"
                                android:paddingStart="@dimen/margin_10dp"
                                android:paddingTop="5dp"
                                android:paddingEnd="@dimen/margin_10dp"
                                android:paddingBottom="5dp"
                                android:text="Manage About Me Data"
                                android:textColor="@color/white"
                                android:textSize="12sp"
                                android:textStyle="bold" />

                        </RelativeLayout>

                    </RelativeLayout>
                </RelativeLayout>
            </androidx.cardview.widget.CardView>


        </LinearLayout>

    </ScrollView>

</RelativeLayout>